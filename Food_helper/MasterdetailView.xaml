<?xml version="1.0" encoding="utf-8" ?><MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"             x:Class="Food_helper.MasterdetailView"                  xmlns:customControlls="clr-namespace:Food_helper.CustomControlls">    <MasterDetailPage.Master>        <ContentPage Title="Menu Recetas">            <StackLayout Orientation="Vertical">                                <!--<Entry HorizontalOptions="FillAndExpand" Placeholder="Ingredientes"/>-->                <SearchBar Placeholder="Ingredientes"/>                <Button Text="Buscar" BackgroundColor="Orange" TextColor="White"/>                <ListView x:Name="lisviewMenu"                          HasUnevenRows="true"                          SeparatorVisibility="Default"                          BackgroundColor="white">                    <ListView.ItemTemplate>                        <DataTemplate>                            <customControlls:CustomViewCellControl SelectedItemBackgroundColor="white">                                <StackLayout Spacing="20" Padding="15" BackgroundColor="white">                                    <Label Text="{Binding Titulo}"                                           TextColor="Black"                                           VerticalOptions="Center"                                           FontSize="Medium"/>                                </StackLayout>                            </customControlls:CustomViewCellControl>                        </DataTemplate>                    </ListView.ItemTemplate>                </ListView>            </StackLayout>        </ContentPage>    </MasterDetailPage.Master>    <MasterDetailPage.Detail>        <NavigationPage>                    </NavigationPage>    </MasterDetailPage.Detail></MasterDetailPage>
